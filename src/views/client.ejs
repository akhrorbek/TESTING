    <title>Login Page</title>
</head>

<body>

    <div id="wrapper">
        <!-- Intro -->
        <section id="intro" class="wrapper style1 fullscreen fade-up">
            <div class="inner container">
                <h1 class="mb-5 text-center">Welcome to Your Future Home üè†</h1>
                <form class="100 d-flex align-items-center"  action="/login" enctype="application/x-www-form-urlencoded">
                    <select   class="m-2" name="companies" id="companies" onchange="changeFunc();">
                        <option selected disabled>Choose company</option>
                        <% allCompanies.map(e => {%>
                            <option value="<%=e.comp_name %>"><%=e.comp_name%></option>
                        <% });%>
                    </select>
                    <select class="m-2" name="" id="">
                        <option value=""></option>

                    </select>
                    <select class="m-2" name="" id="">
                        <option></option>
                    </select>
                    <select class="m-2" name="" id="">
                        <option disabled selected>choose year</option>
                        <option value="10">10</option>
                        <option value="15">15</option>
                        <option value="20">20</option>
                    </select>
                </form>
            </div>
        </section>

    </div>

    <script src="../assets/app.js"></script>

</body>

</html>